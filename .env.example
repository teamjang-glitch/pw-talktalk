# Google OAuth 설정 (Google Cloud Console에서 발급)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# NextAuth 설정
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_key_here

# Google Sheets API 설정
# Service Account JSON을 Base64로 인코딩한 값
GOOGLE_SERVICE_ACCOUNT_KEY=your_base64_encoded_service_account_json

# 스프레드시트 ID (URL에서 /d/ 뒤의 값)
GOOGLE_SPREADSHEET_ID=your_spreadsheet_id

# 허용 도메인
ALLOWED_DOMAIN=spacecloud.kr

# 어드민 이메일 (콤마로 구분)
ADMIN_EMAILS=socialjung@spacecloud.kr,master@spacecloud.kr
